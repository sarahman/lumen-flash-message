{
    "name": "sarahman/lumen-flash-message",
    "description": "This package is meant for using the `laracasts/flash` package into the Lumen based projects.",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "laracasts/flash": "^3.0",
        "illuminate/session": "~5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\Flash\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "lumen": {
            "providers": [
                "Sarahman\\Flash\\FlashServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ]
}
